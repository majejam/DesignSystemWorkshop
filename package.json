  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .eslintignore .",
    "format": "prettier --config .prettierrc --write \"./**/*.+(js|vue|json|yml|yaml|css|scss)\" --ignore-path .eslintignore",
